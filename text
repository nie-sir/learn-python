	O  [label = "县" color=SkyBlue,style=filled,fontcolor=Red,fontsize=24,shape=circle]  // node O
	A->{B,C,D} [label="三公" color=Blue,fontcolor=Red] // edge A->{B,C,D}
	B->{E,F,G,H,I,J,K,L,M} [label="九卿" color=yellowgreen,fontcolor=darkgreen] // edge B->{E,F,G,H,I,J,K,L,M}
	D->{E,F,G,H,I,J,K,L,M} [label="九卿" color="crimson", style="dashed",fontcolor=darkgreen] // edge D->{E,F,G,H,I,J,K,L,M}
	E->N [style="dashed", color="forestgreen"] // edge E->N
	M->N [style="dashed", color="forestgreen"] // edge M->N
	N->O [fontcolor=Black] // edge N->O
}

                          
