	E  [label = "奉常" color=Green,style=filled,fontcolor=Red,fontsize=24,shape=box]  // node E
	F  [label = "郎中令" color=Green,style=filled,fontcolor=Red,fontsize=24,shape=box]  // node F
	G  [label = "衙蔚" color=Green,style=filled,fontcolor=Red,fontsize=24,shape=box]  // node G
